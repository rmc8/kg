{
  "mcpServers": {
    "jupyter": {
      "command": "uv",
      "args": [
        "run",
        "jupyter-mcp-server"
      ],
      "env": {
        "SERVER_URL": "http://localhost:8888",
        "TOKEN": "MY_TOKEN",
        "NOTEBOOK_PATH": "notebooks/main.ipynb"
      }
    },
    "gemini-cli": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@choplin/mcp-gemini-cli",
        "--allow-npx"
      ],
      "env": {}
    }
  }
}